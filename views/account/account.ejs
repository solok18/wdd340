<%# Content Heading %>
 <% if (title) { %>
<h1><%= title %></h1>
<% } else {
    res.redirect('/')
} %>

<%# flash message %>
<% if (message) {%>
<div class="flash-message">
    <p><%= message %></p>
</div><% } %>


<%# Error Display %>
<% if (errors) { %>
  <ul class="notice">
 <% errors.array().forEach(error => { %>
   <li><%= error.msg %></li>
<%  }) %>
 </ul>
<% } %>

<p class="loginConfirm"> You're logged in.</p>